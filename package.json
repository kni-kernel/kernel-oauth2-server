{
  "name": "kernel-oauth2-server",
  "version": "1.0.0",
  "description": "OAUTH2 Server for Kernel ",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run eslint",
    "serve": "node dist/app.js",
    "watch": "nodemon dist/app.js",
    "test": "jest --coverage --passWithNoTests",
    "reformat": "npm run prettier && npm run eslint",
    "prettier": "prettier --write '**/*.ts'",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "eslint": "tsc --noEmit && eslint 'src/**/*.{js,ts,tsx}' --fix",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon $NODE_DEBUG_OPTION dist/app.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "author": "Piotr Moszkowicz",
  "license": "MIT",
  "_moduleAliases": {
    "@": "dist/",
    "@middleware": "dist/middleware/",
    "@models": "dist/models/",
    "@services": "dist/services/",
    "@util": "dist/util/",
    "@views": "dist/views/"
  },
  "dependencies": {
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "config": "^3.2.5",
    "cookie-parser": "^1.4.4",
    "csurf": "^1.11.0",
    "ejs": "^2.7.1",
    "express": "^4.17.1",
    "express-oauth-server": "^2.0.0",
    "module-alias": "^2.2.2",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.4",
    "sequelize-typescript": "^1.1.0",
    "winston": "^3.2.1",
    "winston-sentry-raven-transport": "^1.2.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/express": "^4.17.2",
    "@types/node": "^13.7.1",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.20.0",
    "concurrently": "^5.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "typescript": "^3.6.4"
  }
}
